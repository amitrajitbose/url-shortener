    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
